<odoo>
    <record model="ir.ui.view" id="iap_account_form_view">
        <field name="name">iap.account.form (in sms_clickatell)</field>
        <field name="model">iap.account</field>
        <field name="inherit_id" ref="iap.iap_account_view_form" />
